{
  "name": "no_idea_cs3249_project",
  "version": "1.0.0",
  "description": "UI project in CS3249",
  "main": "server.babel.js",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "cross-env NODE_ENV=development nodemon server/server.js",
    "build:client": "cross-env NODE_ENV=production webpack",
    "build:server": "babel -d ./build ./server -s && cp ./server/index.html ./build/index.html && ./server/favicon.ico ./build/favicon.ico",
    "clean": "rm -rf build dist",
    "build:prod": "npm run clean && npm run build:client && npm run build:server",
    "start:prod": "cross-env NODE_ENV=production node build/server.js",
    "debug": "node --debug ./build/index.js",
    "db-setup": "node dbSetup.babel.js",
    "lint": "eslint client server dbSetup.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "jsdoc": "./node_modules/.bin/jsdoc --configure docs/config/jsdoc.json -r --verbose",
    "serve-docs": "./node_modules/.bin/live-server ./docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamalsegard/NoIdea_CS3249_Project.git"
